{
    "package": {
        "dependencies": {
            "antd": "^4.6.3",
            "react": "^16.13.1",
            "react-dom": "^16.13.1"
        },
        "scripts": {
            "husky-run": "node_modules/.bin/husky-run",
            "prettier": "node_modules/.bin/prettier",
            "eslint": "node_modules/.bin/eslint",
            "start": "react-scripts start",
            "build": "react-scripts build",
            "test": "react-scripts test",
            "eject": "react-scripts eject"
        },
        "husky": {
            "hooks": {
                "pre-commit": "pretty-quick --staged --pattern '**/*.js' --pattern '**/*.html' "
            }
        },
        "devDependencies": {
            "@testing-library/jest-dom": "^4.2.4",
            "@testing-library/react": "^9.3.2",
            "@testing-library/user-event": "^7.1.2",
            "react-scripts": "3.4.3",
            "babel-eslint": "^10.1.0",
            "eslint": "^7.7.0",
            "eslint-import-resolver-alias": "^1.1.2",
            "eslint-plugin-import": "^2.22.0",
            "husky": "^4.2.5",
            "lint-staged": "^10.2.13",
            "prettier": "^2.1.1",
            "pretty-quick": "^3.0.0",
            "eslint-plugin-jsx-a11y": "^6.2.3"
        },
        "engines": {
            "node": ">=12.0"
        },
        "eslintConfig": {
            "parser": "babel-eslint",
            "parserOptions": {
                "ecmaVersion": 8,
                "sourceType": "module",
                "ecmaFeatures": {
                    "experimentalObjectRestSpread": true
                }
            },
            "extends": [
                "eslint:recommended",
                "react-app",
                "plugin:jsx-a11y/recommended"
            ],
            "plugins": [
                "jsx-a11y"
            ],
            "env": {
                "node": true,
                "jest": true,
                "commonjs": true,
                "es6": true
            },
            "rules": {
                "no-unused-vars": 2,
                "no-useless-escape": 0,
                "no-extra-semi": 1,
                "no-var": 2,
                "no-new-object": 2,
                "object-shorthand": 1,
                "no-array-constructor": 1,
                "array-callback-return": 1,
                "prefer-destructuring": 1,
                "quotes": "single",
                "prefer-rest-params": 1,
                "no-param-reassign": 1,
                "one-var": 1,
                "no-case-declarations": 1,
                "no-nested-ternary": 1,
                "no-else-return": 1,
                "no-new-wrappers": 1,
                "id-length": [
                    1,
                    {
                        "min": 3
                    }
                ],
                "camelcase": [
                    2,
                    {
                        "properties": "always",
                        "ignoreDestructuring": false,
                        "ignoreImports": false,
                        "ignoreGlobals": false
                    }
                ],
                "new-cap": [
                    1,
                    {
                        "newIsCap": true,
                        "capIsNew": true,
                        "properties": true
                    }
                ],
                "react/jsx-uses-react": 0,
                "react/react-in-jsx-scope": 0,
                "react/jsx-uses-vars": 1,
                "react/display-name": 0,
                "react-hooks/exhaustive-deps": 0
            }
        },
        "browserslist": {
            "production": [
                ">0.2%",
                "not dead",
                "not op_mini all"
            ],
            "development": [
                "last 1 chrome version",
                "last 1 firefox version",
                "last 1 safari version"
            ]
        }
    }
}